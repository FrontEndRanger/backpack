<h2>Forms</h2>
<h3>Inputs</h3>
<div class="sg-example">
  <form action="">
    <label for="">A label</label>
    <input type="text" placeholder="Type your text here">
    <label for="">Textarea</label>
    <textarea name="" id="" cols="30" rows="10" placeholder="Type your text here"></textarea>
    <label for="">Select</label>
    <select name="" id="">
      <option value="">Option 1</option>
      <option value="">Option 2</option>
      <option value="">Option 3</option>
    </select>
    <label for="">Checkboxes</label>
    <input type="checkbox"><label for="">Checkbox 1</label>
    <input type="checkbox"><label for="">Checkbox 2</label>
    <input type="checkbox"><label for="">Checkbox 3</label>
    <label for="">Radio buttons</label>
    <input type="radio"><label for="">Radio 1</label>
    <input type="radio"><label for="">Radio 2</label>
    <input type="radio"><label for="">Radio 3</label>
    <label for="">Password</label>
    <input type="password">
    <label for="">Datetime</label>
    <input type="datetime">
    <label for="">Phone</label>
    <input type="phone">
    <label for="">File</label>
    <input type="file">
    <label for="">Progress</label>
    <progress></progress>
    <fieldset>
      <legend>The legendary fieldset</legend>
      <label for="">Mr. Label</label>
      <input type="text" placeholder="Type your text here">
    </fieldset>
  </form>
</div>
